{
    "manifestVersion": 1,
    "id": "priaAzure",
    "version": "2.0.18",
    "name": "PR Inspection Assistant Fork(Azure OpenAI)",
    "publisher": "OruchReis",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Automate pull request reviews in Azure DevOps using OpenAI. This bot analyzes code changes, offers suggestions, detects potential bugs, and ensures adherence to coding standards.",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "PR",
        "Code Analysis",
        "Open AI",
        "OpenAI",
        "Code Quality",
        "ChatGPT",
        "Quality Assurance",
        "Pull Request",
        "Pipeline",
        "Code Inspection",
        "Peer Review",
        "Code Review",
        "AI"
    ],
    "icons": {
        "default": "assets/logo.png"
    },
    "branding": {
        "color": "rgb(138, 43, 226)",
        "theme": "dark"
    },
    "links": {
        "support": {
            "uri": "https://github.com/oruchreis/pr-inspection-assistant",
            "version": "2.0.14"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/oruchreis/pr-inspection-assistant"
    },
    "screenshots": [
    ],
    "content": {
        "details": {
            "path": "assets/overview.md"
        }
    },
    "files": [
        {
            "path": "src"
        },
        {
            "path": "assets/overview.md",
            "addressable": true
        },
        {
            "path": "assets/logo.png",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "src"
            }
        }
    ]
}